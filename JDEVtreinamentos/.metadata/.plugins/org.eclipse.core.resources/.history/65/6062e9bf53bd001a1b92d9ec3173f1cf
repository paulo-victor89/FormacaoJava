package LogicaEAlgoritmos;

public class OperadoresRelacionais {

	public static void main(String[] args) {

		int n1 = 10;
		
		int n2 = 20;
		
		//Igual a 
//		if(n1 == n2 ) {
//			System.out.println("Os numeros são iguais");
//		}else {
//			System.out.println("Os numeros são diferentes");
//		}
		//Diferente de
		if(n1 != n2 ) {
			System.out.println("Os numeros são diferentes");
		}else {
			System.out.println("Os numeros são Iguais");
		}
	}

}
